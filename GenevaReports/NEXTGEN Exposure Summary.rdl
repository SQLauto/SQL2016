        <CommandText>SELECT
          DECODE(:Group1,
          "InvestmentType",
          {Investment.InvestmentType.Description},
          "AssetType",
          {Investment.AssetType.Description},
          "Counterparty",
          {LocationAccount.NameLine1},
          "LocalBasisCurrency",
          {BasketInvestment.BifurcationCurrency.Description},
          "PrintGroup",
          {Investment.PrintGroup.Description},
          "SubIndustry",          {Investment.SubIndustry.Description},
          "Industry",             {Investment.SubIndustry.Industry.Description},
          "IndustryGroup",        {Investment.SubIndustry.Industry.IndustryGroup.Description},
          "Sector",               {Investment.SubIndustry.Industry.IndustryGroup.Sector.Description},
          "SubIndustry2",          {Investment.SubIndustry2.Description},
          "Industry2",             {Investment.SubIndustry2.Industry.Description},
          "IndustryGroup2",        {Investment.SubIndustry2.Industry.IndustryGroup.Description},
          "Sector2",               {Investment.SubIndustry2.Industry.IndustryGroup.Sector.Description},
          "Country",              {Investment.IssueCountry.Description},
          "Issuer",              {Investment.Issuer.NameSort},
          "None","") Group1,

          DECODE(:Group2,
          "InvestmentType",
          {Investment.InvestmentType.Description},
          "AssetType",
          {Investment.AssetType.Description},
          "Counterparty",
          {LocationAccount.NameLine1},
          "LocalBasisCurrency",
          {BasketInvestment.BifurcationCurrency.Description},
          "PrintGroup",
          {Investment.PrintGroup.Description},
          "SubIndustry",          {Investment.SubIndustry.Description},
          "Industry",             {Investment.SubIndustry.Industry.Description},
          "IndustryGroup",        {Investment.SubIndustry.Industry.IndustryGroup.Description},
          "Sector",               {Investment.SubIndustry.Industry.IndustryGroup.Sector.Description},
          "SubIndustry2",          {Investment.SubIndustry2.Description},
          "Industry2",             {Investment.SubIndustry2.Industry.Description},
          "IndustryGroup2",        {Investment.SubIndustry2.Industry.IndustryGroup.Description},
          "Sector2",               {Investment.SubIndustry2.Industry.IndustryGroup.Sector.Description},
          "Country",              {Investment.IssueCountry.Description},
          "Issuer",              {Investment.Issuer.NameSort},
          "None","") Group2,
          DECODE(:Group1,
          "InvestmentType",
          {Investment.InvestmentType.Code},
          "AssetType",
          {Investment.AssetType.Code},
          "Counterparty",
          {LocationAccount.NameSort},
          "Currency",
          {BasketInvestment.BifurcationCurrency.Code}
          )GroupCode,
          Decode({BasketInvestment.IsSwap} ,
          1, {Portfolio.AccountingParameters.BookCurrency.Description},
          Decode ({BasketInvestment.IsOption} OR {BasketInvestment.IsWarrant} OR {BasketInvestment.IsFuture},
          1,  {BasketInvestment.UnderlyingInvestment.Description} ,
          Decode({BasketInvestment.IsForwardCash},
          1,  DECODE(bisTaxLot=&gt;{TaxLotTransaction.PortfolioEvent.Investment.BifurcationCurrency.Description} == {BasketInvestment.Description },
          1, {BasketInvestment.Description },
          {Portfolio.AccountingParameters.BookCurrency.Description}),
          DECODE({BasketInvestment.IsForwardFXContract},
          1,DECODE({BasketInvestment.BuyCurrency.Description} == {Investment.Description},
          1,{BasketInvestment.BuyCurrency.Description} ,
          {Portfolio.AccountingParameters.BookCurrency.Description}),
          {Investment.Description}))))  ExposureInvest,


          Sum(Balance(Net,Book,PeriodEnd,:InventoryState,MktValSummary)
          - Balance(Net,Book,PeriodEnd,InDefault,AllAmortizationAssetLiability)
          + Balance(Net,Book,PeriodEnd,All,AllAccruedInterest)
          - Balance(Net,Book,PeriodEnd,InDefault,AllAccruedInterest)) MktVal,

          Sum(Balance(Long,Book,PeriodEnd,:InventoryState,MktValSummary)
          - Balance(Long,Book,PeriodEnd,InDefault,AllAmortizationAssetLiability)
          + Balance(Long,Book,PeriodEnd,All,AllAccruedInterest)
          - Balance(Long,Book,PeriodEnd,InDefault,AllAccruedInterest))  LongMval,

          Sum(Balance(Short,Book,PeriodEnd,:InventoryState,MktValSummary)
          - Balance(Short,Book,PeriodEnd,InDefault,AllAmortizationAssetLiability)
          + Balance(Short,Book,PeriodEnd,All,AllAccruedInterest)
          - Balance(Short,Book,PeriodEnd,InDefault,AllAccruedInterest)) ShortMval,




          {LocationAccount.NameLine1} Counterparty,

          {Portfolio.AccountingParameters.BookCurrency.Code} BookCurrency
FROM	bisLocalPosition
GIVEN  Portfolio = :Portfolio,
  AccountingPeriodStartDate = :PeriodStartDate,
  AccountingPeriodEndDate = :PeriodEndDate,
  KnowledgeDate = :KnowledgeDate,
  PriorKnowledgeDate = :PriorKnowledgeDate,
  AccountingFilters = :AccountingFilters,
  AccountingParameters = :AccountingParameters,
  ConsolidateLegalEntities = :ConsolidateLegalEntities,
  AccountingRule = :AccountingRule,
  AccountingRunType = :AccountingRunType,
  AccountingView = :AccountingView,
  AccrueOnSTIF = :AccrueOnSTIF,
  AccrueWithhold = :AccrueWithhold,
  AmortizationElection = :AmortizationElection,
  AmortizeOnUnrealizedDate = :AmortizeOnUnrealizedDate,
  AppraisalDay = :AppraisalDay,
  BidAskClosing = :BidAskClosing,
  BidAskPricing = :BidAskPricing,
  BookCurrency = :BookCurrency,
  BusinessCalendar = :BusinessCalendar,
  ByPassRule = :ByPassRule,
  CounterClosingRuleOverride = :CounterClosingRuleOverride,
  CrossingCurrency = :CrossingCurrency,
  CrossZeroOnCoverShort = :CrossZeroOnCoverShort,
  CrossZeroOnDebt = :CrossZeroOnDebt,
  CrossZeroOnEquity = :CrossZeroOnEquity,
  CrossZeroOnFutures = :CrossZeroOnFutures,
  CrossZeroOnOptions = :CrossZeroOnOptions,
  ExcludeCommission = :ExcludeCommission,
  FuturesClosingRuleOverride = :FuturesClosingRuleOverride,
  InvestmentClosingRuleOverride = :InvestmentClosingRuleOverride,
  LumpCashLots = :LumpCashLots,
  LumpNonCashLots = :LumpNonCashLots,
  MarkToMarketCashTransfer = :MarkToMarketCashTransfer,
  PostDividendTaxLotDetail = :PostDividendTaxLotDetail,
  PriceSchedule = :PriceSchedule,
  SecondaryCounterClosingRuleOverride = :SecondaryCounterClosingRuleOverride,
  SecondaryFuturesClosingRuleOverride = :SecondaryFuturesClosingRuleOverride,
  SecondaryInvestmentClosingRuleOverride = :SecondaryInvestmentClosingRuleOverride,
  SegregateIncome = :SegregateIncome,
  SettleFXCost = :SettleFXCost,
  WithholdPurchasedSoldAI = :WithholdPurchasedSoldAI,
  LegalEntity = :FundLegalEntity,
  MarkLegalEntity = :MarkLegalEntity,
  DisableFreezepoints = :DisableFreezepoints,
  DisableBooks = :DisableBooks,
  DisableLockdowns = :DisableLockdowns,
  DisableLDErrorRead = :DisableLDErrorRead,
  DisableManualJE = :DisableManualJE,
  DisableSnapshots = :DisableSnapshots,
  EnableRealTimePricing = :EnableRealTimePricing,
  PostValidationErrors = :PostValidationErrors,
  SnapshotUse = :SnapshotUse,
  BISQueryType = "PeriodActivity",
  LumpCrossGL = :LumpCrossGL,
  GainCalcMethod = :GainCalcMethod,
  FiscalCalendar = :FiscalCalendar
Where LongMval!=0 or ShortMval!=0
GROUP BY Group1, Group2, ExposureInvest</CommandText>
