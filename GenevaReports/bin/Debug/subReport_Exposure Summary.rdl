    Private colorPalette As String() = {"#4B8DA4", "#F1E73f", "#9DD3D5", "#9E9E73", "#A4D077", "#6A6A6C", "#69B9AA", "#ECC043", "#6DC497", "#9D689B", "#AD8A44", "#C63070", "#AFB7AB", "#DFE056", "#EDA563"}

    Private count As Integer = 0

    Private mapping As New System.Collections.Hashtable()

    Public Function GetColor(ByVal groupingValue As String) As String
    If groupingValue = "" Then
    Return "#82BBE7"
    End If

    If mapping.ContainsKey(groupingValue) Then
    Return mapping(groupingValue)
    End If
    Dim c As String = colorPalette(count Mod colorPalette.Length)
    count = count + 1
    mapping.Add(groupingValue, c)
    Return c
    End Function


public DynamicHeight as String = ""
private height as Single = 0

public function InitCountGroup(count As Integer) as boolean
if count &lt; 10 then 
  height = count * 0.45
  else
  height = count * 0.25
  end if

  DynamicHeight = CStr(height) + " in"

  return false
